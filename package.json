{
  "name": "@zdmin/crab",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "prepublishOnly": "run-s build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuyimingwork/zdmin-crab.git"
  },
  "bugs": {
    "url": "https://github.com/xuyimingwork/zdmin-crab/issues"
  },
  "homepage": "https://github.com/xuyimingwork/zdmin-crab#readme",
  "author": "Xu Yiming",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-oxlint": "~1.8.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.8.0",
    "sass-embedded": "^1.90.0",
    "typescript": "~5.8.0",
    "unplugin-auto-import": "^20.0.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-bundle-analyzer": "^1.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-inspect": "^11.3.2",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.4"
  },
  "dependencies": {
    "easy-bem": "^1.1.1",
    "es-toolkit": "^1.39.8",
    "vue-asyncx": "^1.5.4"
  }
}